{
	"name": "@tsuki/theia",
	"version": "0.0.1",
	"main": "./dist/bin/index.js",
	"description": "CLI for bootstrapping opinionated tsuki-style typescript projects, from tsconfigs to fullstack boilerplates & monorepo support!",
	"repository": "https://github.com/MoonTory/tsuki-cli.git",
	"author": "Gustavo Quinta | MoonTory <gustavoqnt40@gmail.com>",
	"license": "MIT",
	"keywords": [
		"cli",
		"typescript",
		"nodejs",
		"reactjs",
		"eslint",
		"bootstrap",
		"boilerplates",
		"tsconfig"
	],
	"bin": {
		"@tsuki/theia": "./dist/bin/index.js",
		"theia": "./dist/bin/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rimraf ./dist && tsc",
		"build:watch": "rimraf ./dist && tsc -watch",
		"test": "mocha -r ts-node/register tests/**/*.test.ts",
		"coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
	},
	"devDependencies": {
		"@types/chai": "^4.2.5",
		"@types/inquirer": "^6.5.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.6.8",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"chai": "^4.2.0",
		"eslint": "^6.1.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"mocha": "^6.2.2",
		"nyc": "^14.1.1",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0",
		"prettier-eslint-cli": "^5.0.0",
		"rimraf": "^2.6.3",
		"ts-node": "^8.3.0",
		"ts-node-dev": "^1.0.0-pre.40",
		"typescript": "^3.5.3"
	},
	"dependencies": {
		"arg": "^4.1.1",
		"core-js": "^3.1.4",
		"inquirer": "^7.0.0"
	}
}
